                <CommandText>SELECT v.Fecha, cl.Apellido + ' ' + cl.Nombre AS Cliente, v.tipoFactura AS 'Tipo', SUBSTRING(RIGHT(REPLICATE('0', 13) + CAST(v.numFactura AS varchar), 13), 0, 6) + '-' + SUBSTRING(RIGHT(REPLICATE('0', 13) + CAST(v.numFactura AS varchar), 13), 6, 13) AS 'Ndecomprobante', 
             SUM(iv.total) AS 'Venta', SUM(iv.cantidad * iv.costoArticulo) AS 'CostoVenta'
FROM   Ventas AS v INNER JOIN
             Documento AS d ON d.NumDoc = v.idCliente INNER JOIN
             Cliente AS cl ON cl.IdCliente = d.IdCliente INNER JOIN
             ItemVenta AS iv ON iv.numFactura = v.numFactura AND iv.tipofactura = v.tipoFactura
WHERE (iv.esarticulo = 0) AND (iv.tipofactura LIKE 'F_') AND (v.anulada = 0)
GROUP BY v.Fecha, cl.Apellido, cl.Nombre, v.tipoFactura, v.numFactura
ORDER BY v.Fecha, Cliente</CommandText>
